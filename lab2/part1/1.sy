int i;
int aa[10];

int max(int a, int b)
{
    if (a >= b)
    {
        return a;
    }
    else
    {
        return b;
    }
}

int main()
{
    int n, f;
    n=getint();
    i = 2;
    f = 1;
    while (i <= n)
    {
        f = f * i;
        i = i + 1;
    }
    putint(f);
    aa[7] = 10;
    putint(max(aa[7], f));
    return 0;
}